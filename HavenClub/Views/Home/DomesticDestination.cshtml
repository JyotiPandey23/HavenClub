<script>
              function filterSelection() {
        var checkboxes = document.querySelectorAll('.cities');
        var selectedValues = [];

        // Collect selected checkbox values
        checkboxes.forEach(function(checkbox) {
            if (checkbox.checked) {
                selectedValues.push(checkbox.value);
            }
        });

        // Debugging: Log the selected values to the console
        console.log("Selected Values:", selectedValues);

        var images = document.getElementsByClassName("listingItem");
          

        console.log(images,'x')
        // Hide all images first
       for (var i = 0; i < images.length; i++) {

           console.log(images[i].classList)
       images[i].style.visibility = "hidden";
    }

        // Show images that match selected values
        if (selectedValues.length === 0) {
        for (var i = 0; i < images.length; i++) {
            images[i].style.display = "visible"; // Show all images
        }
        return; // Exit the function
    }

        for (var i = 0; i < images.length; i++) {
            for (var j = 0; j < selectedValues.length; j++) {
                if (images[i].classList.contains(selectedValues[j])) {
                images[i].style.visibility = "visible";
            }
            }
        }
    }

    // Show filtered elements
    function w3AddClass(element, name) {
        var arr1 = element.className.split(" ");
        if (arr1.indexOf(name) == -1) {
            element.className += " " + name;
        }
    }

    // Hide elements that are not selected
    function w3RemoveClass(element, name) {
        var arr1 = element.className.split(" ");
        while (arr1.indexOf(name) > -1) {
            arr1.splice(arr1.indexOf(name), 1);
        }
        element.className = arr1.join(" ");
    }

    </script>

<script type="text/javascript">

    $(document).ready(function(){

        filter_data();

        function filter_data(page_no) {

            // $('.loader-cart').show();
            var cities_arr = get_cities();
            var category = 'domestic';

             $.ajax({
                url:'https://www.clubliberty.in/home/get_resorts',
                method:"POST",
                data:{cities_arr:cities_arr, page_no:page_no, category:category},
                success:function(data){
                 // alert(data);
                 // $('.loader-cart').hide();
                    $('#filtered_data').html(data);
                }
            });
        }

        function get_cities() {
            var arr = [];
            $('.cities:checked').each(function(){
                arr.push($(this).val());
            });
            return arr;
        }

        $('.cities').click(function(){
            filter_data();
        });

        $(document).on('click','a.paginate', function(e){
            e.preventDefault();
            var page_id = $(this).attr('id');
            filter_data(page_id);
        });

    });

</script>
